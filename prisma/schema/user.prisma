
model User {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  age          Int?
  avatar       String?
  birthday     String?
  height       Int?
  introduction String?
  name         String?
  phoneNumber  String?
  sex          Int?
  weight       Int?

}

model Follow {
   id           String   @id @default(auto()) @map("_id") @db.ObjectId

}

model Post {
   id           String   @id @default(auto()) @map("_id") @db.ObjectId

}

model Comment {
   id           String   @id @default(auto()) @map("_id") @db.ObjectId

}

model Like {
   id           String   @id @default(auto()) @map("_id") @db.ObjectId

}