

type NeedsPalLimits {
  minAge Int?
  number Int?
  sex    Int?
}


model Need {
  id            String         @id @default(auto()) @map("_id") @db.ObjectId
  creatorId     String?
  description   String?
  images        String[]
  keywords      String?
  location      String?
  palIds        String[]
  palLimits     NeedsPalLimits?
  paymentMethod Int?
  time          String?
}
